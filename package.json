{
  "name": "tsonify",
  "version": "1.0.0",
  "description": "TSON Helper Library",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.es.js",
  "types": "dist/bundle.d.ts",
  "repository": "https://github.com/spectral-discord/TSON.git",
  "author": "Lina <git@lina.pizza>",
  "license": "Unlicense",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && yarn rollup -c",
    "lint": "eslint src --fix",
    "lint-staged": "lint-staged",
    "test": "jest --config jest.config.ts",
    "preversion": "yarn test && yarn lint",
    "prepublishOnly": "yarn test && yarn lint",
    "prepare": "husky install",
    "build-schema": "ts-node -P tsconfig.scripts.json ./scripts/build-json-schema.ts"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.0.1",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.1",
    "husky": "^8.0.0",
    "jest": "^29.0.3",
    "joi-to-json": "^2.6.1",
    "json": "^11.0.0",
    "lint-staged": "^13.0.3",
    "npm-add-script": "^1.1.0",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^4.2.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@types/joi": "^17.2.3",
    "@types/mathjs": "^9.4.2",
    "@types/yaml": "^1.9.7",
    "joi": "^17.6.0",
    "mathjs": "^11.2.1",
    "yaml": "^2.1.1"
  }
}
